inscripcion = "Durante su expedición, los exploradores encuentran una inscripción antigua en un templo."


frecuencia_letras = {}

for letra in inscripcion:
    
    if letra.isalpha():
        
        letra = letra.lower()
        
        if letra in frecuencia_letras:
            frecuencia_letras[letra] += 1
        else:
            frecuencia_letras[letra] = 1


print(frecuencia_letras)


